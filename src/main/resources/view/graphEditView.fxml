<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<HBox xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="com.example.controller.GraphEditController">

    <Button text = "Vertex" fx:id = "vertexButton"/>
    <Button text = "Edge" fx:id = "edgeButton"/>
    <Button text = "Delete" fx:id = "deleteButton"/>
    <Button text = "None" fx:id = "noneButton"/>
    <Button text = "Undo" fx:id = "undoButton"/>
    <Button text = "Redo" fx:id = "redoButton"/>
    <Button text = "Simulate" fx:id = "simulateButton"/>
    <Button text = "Draw" fx:id = "drawButton"/>
</HBox>
